<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
         http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0"
         metadata-complete="true">

    <servlet>
        <servlet-name>UserServlet</servlet-name>
        <servlet-class>cn.fcsanf.web.servlet.UserServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UserServlet</servlet-name>
        <url-pattern>/user</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>DataServlet</servlet-name>
        <servlet-class>cn.fcsanf.web.servlet.DataServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DataServlet</servlet-name>
        <url-pattern>/DataServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ShowDataServlet</servlet-name>
        <servlet-class>cn.fcsanf.web.servlet.ShowDataServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ShowDataServlet</servlet-name>
        <url-pattern>/ShowDataServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>NowDataServlet</servlet-name>
        <servlet-class>cn.fcsanf.web.servlet.NowDataServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>NowDataServlet</servlet-name>
        <url-pattern>/NowDataServlet</url-pattern>
    </servlet-mapping>


    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>

</web-app>